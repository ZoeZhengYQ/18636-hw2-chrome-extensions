{
    "name": "Send Location",
    "description": "Send geolocation every minute",
    "version": "2.0",
    "permissions": [
      "activeTab",
      "geolocation"
    ],
    "background": {
      "scripts": ["background.js", "jquery-3.4.1.js"],
      "persistent": false
    },
    "browser_action": {
      "default_title": "Send Geolocation"
    },
    "manifest_version": 2
  }
  